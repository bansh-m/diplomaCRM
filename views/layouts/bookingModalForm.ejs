<div class="modal fade" id="slotModal" tabindex="-1" role="dialog" aria-labelledby="slotModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="slotModalLabel">New Booking</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <form id="bookingForm">
            <div class="form-group">
              <label for="clientName">Client Name</label>
              <input type="text" class="form-control" id="clientName" name="clientName" required>
            </div>
            <div class="form-group">
              <label for="clientContact">Client Contact</label>
              <input type="text" class="form-control" id="clientContact" name="clientContact" required>
            </div>
            <input type="hidden" id="slotId" name="slotId">
          </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          <button type="button" class="btn btn-primary" id="saveBooking">Save changes</button>
        </div>
      </div>
    </div>
  </div>
  